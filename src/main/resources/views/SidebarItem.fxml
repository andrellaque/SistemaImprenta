<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="166.0" spacing="10.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pe.edu.utp.sistemaimprenta.controller.SidebarItemController">
    <padding>
        <Insets bottom="10.0" left="20.0" top="10.0" />
    </padding>
    <children>
        <ImageView fx:id="icon" fitHeight="30.0" fitWidth="30.0" preserveRatio="true" />
        <Label fx:id="label" styleClass="menu-label">
         <font>
            <Font name="Segoe UI" size="16.0" />
         </font></Label>
    </children>
</HBox>
